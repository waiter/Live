<StyleSheet>
    <Style name="contentLableStyle">
        <Setter property="textColor" value="{{color:kt_theme_secondary_textcolor}}" />
        <Setter property="font" value="{{font:R28}}" />
    </Style>
    <Style name="clipsViewStyle">
        <Setter property="clipsToBounds" value="YES"/>
    </Style>
    <Style name="titleLableStyle">
        <Setter property="textColor" value="{{color:kt_theme_title_textcolor}}"></Setter>
        <Setter property="font" value="{{font:R32}}"></Setter>
        <Setter property="normalTextColor" value="{{color:kt_theme_title_textcolor}}"></Setter>
        <Setter property="backgroundColor" value="clear"></Setter>
    </Style>
    <Style name="contentLableStyle">
        <Setter property="normalTextColor" value="{{color:kt_theme_secondary_textcolor}}"></Setter>
        <Setter property="textColor" value="{{color:kt_theme_secondary_textcolor}}"></Setter>
        <Setter property="font" value="{{font:R28}}"></Setter>
        <Setter property="hyperLinkColor" value="{{color:kt_theme_color}}"></Setter>
        <Setter property="backgroundColor" value="clear"></Setter>
    </Style>
</StyleSheet>
